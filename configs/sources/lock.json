        "signature_url": "https://archive.mariadb.org/mariadb-11.8.6/bintar-linux-systemd-x86_64/mariadb-11.8.6-linux-systemd-x86_64.tar.gz.asc",
        "public_key_fingerprint": "177F4010FE56CA3336300305F1656F24C74CD1D8",
        "build": {
          "commands": [
            "cp -a . {{install_dir}}/"
          ]
        },
        "systemd": {
          "name": "aipanel-runtime-mariadb.service",
          "exec_start": "{{install_dir}}/bin/mariadbd --basedir={{install_dir}} --datadir={{install_dir}}/data --plugin-dir={{install_dir}}/lib/plugin --user=root"
        }
      }
