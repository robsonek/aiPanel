        "public_key_fingerprint": "1729F83938DA44E27BA0F4D3DBDB397470D12172",
        "build": {
          "commands": [
            "./configure --prefix={{install_dir}} --enable-fpm --with-fpm-user=www-data --with-fpm-group=www-data --with-zlib --with-openssl",
            "make -j$(nproc)",
            "make install"
          ]
        },
        "systemd": {
          "name": "aipanel-runtime-php-fpm.service",
          "exec_start": "{{runtime_dir}}/php-fpm/current/sbin/php-fpm --nodaemonize"
        }
