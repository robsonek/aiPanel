{
  "schema_version": 1,
  "channels": {
    "stable": {
      "nginx": {
        "version": "1.29.5",
        "source_url": "https://nginx.org/download/nginx-1.29.5.tar.gz",
        "source_sha256": "6744768a4114880f37b13a0443244e731bcb3130c0a065d7e37d8fd589ade374",
        "signature_url": "https://nginx.org/download/nginx-1.29.5.tar.gz.asc",
        "public_key_fingerprint": "43387825DDB1BB97EC36BA5D007C8D7C15D87369",
        "build": {
          "commands": [
            "./configure --prefix={{install_dir}} --with-http_ssl_module --with-http_v2_module",
            "make -j$(nproc)",
            "make install"
          ]
        },
        "systemd": {
          "name": "aipanel-runtime-nginx.service",
          "exec_start": "{{runtime_dir}}/nginx/current/sbin/nginx -g \"daemon off;\"",
          "exec_reload": "/bin/kill -HUP $MAINPID",
          "exec_stop": "/bin/kill -QUIT $MAINPID"
        }
      },
      "php-fpm": {
        "version": "8.5.2",
        "source_url": "https://www.php.net/distributions/php-8.5.2.tar.gz",
        "source_sha256": "379ccccefcc85f28286444cf01bc0db017c79d513417267f5bb1d804b8428c22",
        "signature_url": "https://www.php.net/distributions/php-8.5.2.tar.gz.asc",
        "public_key_fingerprint": "D95C03BC702BE9515344AE3374E44BC9067701A5",
        "build": {
          "commands": [
            "./configure --prefix={{install_dir}} --enable-fpm --with-fpm-user=www-data --with-fpm-group=www-data --with-zlib --with-openssl --with-mysqli=mysqlnd --with-pdo-mysql=mysqlnd --enable-mbstring",
            "make -j$(nproc)",
            "make install"
          ]
        },
        "systemd": {
          "name": "aipanel-runtime-php-fpm.service",
          "exec_start": "{{runtime_dir}}/php-fpm/current/sbin/php-fpm --nodaemonize"
        }
      },
      "mariadb": {
        "version": "11.8.6",
        "source_url": "https://archive.mariadb.org/mariadb-11.8.6/bintar-linux-systemd-x86_64/mariadb-11.8.6-linux-systemd-x86_64.tar.gz",
